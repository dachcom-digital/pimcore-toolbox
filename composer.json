{
    "name": "dachcom-digital/toolbox",
    "type": "pimcore-bundle",
    "license": "GPL-3.0-or-later",
    "description": "Pimcore Project Kickstarter.",
    "keywords": ["pimcore", "toolbox"],
    "homepage": "https://github.com/dachcom-digital/pimcore-toolbox",
    "authors": [
        {
            "name": "DACHCOM.DIGITAL Stefan Hagspiel",
            "email": "shagspiel@dachcom.ch",
            "homepage": "http://www.dachcom.com/",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "ToolboxBundle\\": "src/ToolboxBundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/TestKernel.php"
        ]
    },
    "extra": {
        "pimcore": {
            "bundles": [
                "ToolboxBundle\\ToolboxBundle"
            ]
        }
    },
    "require": {
        "pimcore/pimcore": "^6.0"
    },
    "require-dev": {
        "codeception/codeception": "~2.4.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-symfony": "^0.12",
        "symplify/easy-coding-standard": "^8.0"
    },
    "conflict": {
        "symfony/monolog-bundle": "3.6.0"
    }
}
